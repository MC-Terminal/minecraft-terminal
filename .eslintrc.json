{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always", { "omitLastInOneLineBlock": true }],
        "indent": ["error", "tab", { "SwitchCase": 1 }],
        "no-tabs": ["error", { "allowIndentationTabs": true }]
    },
    "globals": {
        
    }
}
